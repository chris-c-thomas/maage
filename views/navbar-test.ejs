    <!-- JavaScript for dropdown interaction -->
    <script>
      document.addEventListener('DOMContentLoaded', function() {
        const navItems = document.querySelectorAll('.nav-item');

        navItems.forEach(item => {
          const link = item.querySelector('a');

          link.addEventListener('click', function(e) {
            // Prevent default only for dropdown toggles
            if (item.querySelector('.dropdown-content')) {
              e.preventDefault();

              // Toggle active class on the current nav-item
              item.classList.toggle('active');

              // Toggle show class on the dropdown
              const dropdown = item.querySelector('.dropdown-content');
              dropdown.classList.toggle('show');

              // Close other dropdowns
              navItems.forEach(otherItem => {
                if (otherItem !== item) {
                  otherItem.classList.remove('active');
                  const otherDropdown = otherItem.querySelector('.dropdown-content');
                  if (otherDropdown) {
                    otherDropdown.classList.remove('show');
                  }
                }
              });
            }
          });
        });

        // Close dropdowns when clicking outside
        document.addEventListener('click', function(e) {
          if (!e.target.closest('.nav-item')) {
            navItems.forEach(item => {
              item.classList.remove('active');
              const dropdown = item.querySelector('.dropdown-content');
              if (dropdown) {
                dropdown.classList.remove('show');
              }
            });
          }
        });
      });

      // Mobile menu toggle
      function toggleMenu() {
        const mobileNavbar = document.getElementById('mobile-navbar');
        mobileNavbar.classList.toggle('hidden');
      }

      // Mobile submenu toggle
      function toggleSubMenu(element) {
        const menuItems = element.nextElementSibling;
        menuItems.style.display = menuItems.style.display === 'none' ? 'block' : 'none';

        // Toggle chevron icon
        const chevron = element.querySelector('.icon-chevron-down');
        chevron.style.transform = menuItems.style.display === 'none' ? 'rotate(0deg)' : 'rotate(180deg)';
      }
    </script>
